   "source": [
    "num = int(input(\"enter the number:\"))\n",
    "flag=0\n",
    "for i in range(2,num//2):\n",
    "    if (num%i)==0:\n",
    "        flag=1\n",
    "        break\n",
    "if(num==1):\n",
    "    print(num,\"is neither prime nor composite\")\n",
    "elif(flag==1):\n",
    "    print(num,\"is not a prime\")\n",
    "else:\n",
    "    print(num,\"is a prime\")\n",
    "       \n",
    "\n"
